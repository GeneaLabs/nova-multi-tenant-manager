{
    "name": "genealabs/nova-multi-tenant-manager",
    "description": "A Laravel Nova package to manage multi-tenants.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "hyn/multi-tenant": "5.4.*",
        "laravel/framework": "5.8.*",
        "laravel/nova": "2.*",
        "php": ">=7.1.3"
    },
    "require-dev": {
        "fzaninotto/faker": "*",
        "mockery/mockery": "*",
        "orchestra/database": "3.8.*",
        "orchestra/testbench-browser-kit": "3.8.*",
        "orchestra/testbench-dusk": "3.8.*",
        "orchestra/testbench": "3.8.*",
        "php-coveralls/php-coveralls": "*",
        "phpmd/phpmd": "*",
        "phpunit/phpunit": "^7.5",
        "predis/predis": "*",
        "sebastian/phpcpd": "*",
        "symfony/thanks": "*"
    },
    "autoload": {
        "psr-4": {
            "Genealabs\\NovaMultiTenantManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Genealabs\\NovaMultiTenantManager\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
