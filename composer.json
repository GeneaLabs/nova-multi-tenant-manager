{
    "name": "genealabs/nova-multi-tenant-manager",
    "description": "A Laravel Nova package to manage multi-tenants.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Genealabs\\NovaMultiTenantManager\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Genealabs\\NovaMultiTenantManager\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
